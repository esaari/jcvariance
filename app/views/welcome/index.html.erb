<!DOCTYPE html>
<html>
<head>
    <title>Simple Map</title>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
    <style>
        html, body {
            height: 100%;
            margin: 0;
            padding: 0;
        }
        #map {
            height: 100%;
        }
    </style>
</head>
<body>
<div id="map"></div>
<script>
    var map;
    function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
            center: {lat: 40.4309, lng: -74.0234},
            zoom: 13
        });
    }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDlzIIybor_qr9avraWURBRYfaws1s54XM&callback=initMap"
        async defer></script>
</body>
</html>